{% capture css %}
    {% include "css/hero.css" %}
    {% include "css/what-we-do.css" %}
    {% include "css/who-we-are.css" %}
    {% include "css/careers.css" %}
{% endcapture %}

<style>
    {{css | cssmin}}

    h2 {
        color: #4c165f;
        font-size: 56px;
        text-align: center;
        font-weight: 400;
    }

    h2::after {
        content: "";
        height: 5px;
        background: #4c165f;
        width: 25px;
        display: block;
        margin: 0.75rem auto 0;
        border-radius: 0.25rem;
    }
</style>

{% include "partials/_hero.njk" %}

<h2 id="what-we-do">What We Do</h2>

<section class="what-we-do">
    {% for item in what_we_do %}
        <div class="what-we-do__item">
            <div class="what-we-do__item__img">
                <img src="{{item.image | url}}" aria-hidden="true" height="121" width="121" loading="lazy"/>
            </div>

            <h3>{{item.heading}}</h3>

            <div class="what-we-do__item__subheading">
                {{item.subHeading}}
            </div>

            <ul>
                {% for point in item.points %}
                    <li>{{point}}</li>
                {% endfor %}
            </ul>

            <div class="what-we-do__item__blurb">
                <nct-read-more>{{item.blurb}}</nct-read-more>
            </div>
        </div>
    {% endfor %}
</section>

<h2 id="who-we-are">Who We Are</h2>

<ul class="who-we-are">
    {% for bio in who_we_are %}
        <li>
            <div class="bio-card">
                <img class="bio-card__headshot" src="{{bio.image | url}}" loading="lazy" aria-hidden="true"/>

                <div class="bio-card__social">
                    {% for social in bio.socials %}
                        {% assign social_icon = "/assets/img/social-icons/" | append: social.type | append: ".avif" | join | url  %}

                        <a href="{{social.url}}" target="_blank" rel="noopener noreferrer" aria-label="Link to Linkedin profile.">
                            <img src="{{social_icon}}" height="25" width="25" aria-hidden="true" loading="lazy"/>
                        </a>
                    {% endfor %}
                </div>

                <div class="bio-card__title">{{bio.title}}</div>

                <div class="bio-card__name">{{bio.name}}</div>

                <div class="bio-card__bio">{{bio.bio}}</div>
            </div>
        </li>
    {% endfor %}
</ul>

